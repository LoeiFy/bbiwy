{
  "manifest_version": 2,
  "name": "bbiwy",
  "description": "Big brother is watching you",
  "version": "0.1.0",
  "background" : {
    "scripts" : ["background.js"]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "web_accessible_resources": [
    "popup.js",
    "test.js"
  ]
}
