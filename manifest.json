{
  "manifest_version": 2,
  "name": "yukar",
  "description": "yukar",
  "version": "0.1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "yukar",
    "default_popup": "src/index.html"
  },
  "permissions": [
    "storage"
  ],
  "sandbox": {
    "pages": [
      "src/iframe/index.html"
    ]
  }
}
