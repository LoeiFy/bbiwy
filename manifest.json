{
  "manifest_version": 2,
  "name": "yukar",
  "description": "Chrome Extension JavaScript Code Editor",
  "version": "0.1.0",
  "homepage_url": "https://github.com/LoeiFy/yukar",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "yukar",
    "default_popup": "index.html"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "permissions": [
    "storage"
  ],
  "sandbox": {
    "pages": [
      "iframe.html"
    ]
  }
}
